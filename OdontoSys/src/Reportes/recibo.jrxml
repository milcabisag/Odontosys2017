	<queryString>
		<![CDATA[SELECT 
	paciente.nombres AS nombre_paciente,
	paciente.apellidos AS apellido_paciente,
	recibo.monto AS montoRecibo,
	recibo.fecha AS fecha,
	talonario.nro_factura AS nro_factura,
	datos.*
FROM paciente 
    INNER JOIN recibo ON paciente.idPaciente = recibo.fk_paciente
    INNER JOIN factura ON factura.idFactura = recibo.fk_factura
    INNER JOIN talonario ON talonario.idTalonario = factura.fk_talonario JOIN datos
WHERE
    recibo.idRecibo = $P{idRecibo}]]>
	</queryString>
