	<parameter name="idAgenda" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT
	agenda.fecha AS fecha,
	agenda.hora AS hora,
	paciente.nombres AS pac_nombre,
	paciente.apellidos AS pac_apellido,
	paciente.nro_ci AS pac_ci,
	datos.*
FROM
    agenda JOIN paciente ON paciente.idPaciente = agenda.fk_paciente
	JOIN datos
WHERE
     agenda.idAgenda = $P{idAgenda}]]>
