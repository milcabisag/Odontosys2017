	<parameter name="idUsuario" class="java.lang.Integer"/>
	<parameter name="year" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT
	servicio.descripcion AS servicio,
	COUNT(servicio.descripcion) AS total,
	usuario.nombre,
	datos.*
FROM
    odontograma JOIN tratamiento ON odontograma.idodontograma = tratamiento.fk_odontograma
	JOIN servicio ON servicio.idservicio = tratamiento.fk_servicio
	JOIN usuario ON odontograma.fk_usuario = usuario.idusuario
	JOIN datos
WHERE
     MONTH(odontograma.fecha) = $P{mes} AND YEAR(odontograma.fecha) = $P{year} AND odontograma.fk_usuario = $P{idUsuario}
GROUP BY
	servicio.descripcion
ORDER BY servicio.descripcion ASC;]]>
	</queryString>
