	<queryString>
		<![CDATA[SELECT 	
	servicio.descripcion AS nom_servicio,
	detalle_orden_empresa.monto AS monto_desc,
	orden_empresa.monto AS monto_total
FROM
	orden_empresa JOIN detalle_orden_empresa ON detalle_orden_empresa.fk_orden_emp = orden_empresa.idorden_empresa
	JOIN servicio ON servicio.idservicio = detalle_orden_empresa.fk_servicio
	
WHERE
	orden_empresa.idorden_empresa = $P{id_ordenempresa}]]>
	</queryString>
