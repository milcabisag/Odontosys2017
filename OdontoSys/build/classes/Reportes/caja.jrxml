	<queryString>
		<![CDATA[SELECT
	usuario.nombre AS user,
	caja.fecha AS fecha,
	caja.descripMovim AS movimiento,
	caja.tipo AS forma_pago,
	caja.entrada AS entrada,
	datos.*
FROM
    caja JOIN usuario ON usuario.idusuario = caja.fk_usuario
	JOIN datos
WHERE
     datos.iddatos = 1 AND caja.fecha BETWEEN $P{fecha_ini} AND $P{fecha_fin}
ORDER BY caja.idcaja ASC;]]>
	</queryString>
